{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "sql:default",
    "shell:default",
    "opener:default",
    {
      "identifier": "opener:allow-open-url",
      "allow": [
        {
          "url": "https://accounts.google.com/o/oauth2/v2/auth"
        }
      ]
    },
    "store:default"
  ]
}